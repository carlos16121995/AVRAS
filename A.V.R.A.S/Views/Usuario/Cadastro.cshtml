            function FormatarDataPadraoParaIngles(date) {
                var data = date.split("-");
                var ano = data[0];
                var mes = data[1];
                var dia = data[2];
                dia = dia.slice(0, 2);
                date = ano + "-" + mes + "-" + dia;

                return date;
            }
            function FormatarDataInglesParaPadrao(date) {
                var data = date.split("-");
                var dia = data[0];
                var mes = data[1];
                var ano = data[2];

                date = dia + "/" + mes + "/" + ano;

                return date;
            }
